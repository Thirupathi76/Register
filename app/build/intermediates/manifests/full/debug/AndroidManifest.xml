<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thiru.register"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.thiru.register.Register"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.thiru.register.Signup"
            android:label="@string/title_activity_signup" >
        </activity>
        <activity
            android:name="com.thiru.register.NameList"
            android:label="@string/title_activity_name_list" >
        </activity>
        <activity
            android:name="com.thiru.register.Details"
            android:label="@string/title_activity_details" >
        </activity>
        <activity
            android:name="com.thiru.register.Display"
            android:label="@string/title_activity_display" >
        </activity>
        <activity
            android:name="com.thiru.register.Splash"
            android:label="@string/title_activity_splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.thiru.register.Login"
            android:label="@string/title_activity_login" >
        </activity>
    </application>

</manifest>